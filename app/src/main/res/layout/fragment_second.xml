<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondFragment">

    <TextView
        android:id="@+id/textview_second5"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_corner"
        android:outlineAmbientShadowColor="#8C4747"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textview_second"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints"
        tools:text="@tools:sample/cities" />

    <Button
        android:id="@+id/button_second3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetch"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview_second"
        app:layout_constraintVertical_bias="0.748" />

    <TextView
        android:id="@+id/textview_second"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_corner"
        android:outlineSpotShadowColor="#E19595"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textview_second4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_second5"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints"
        tools:text="@tools:sample/cities" />

    <TextView
        android:id="@+id/textview_second2"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_corner"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textview_second3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_second4"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints"
        tools:text="@tools:sample/cities" />

    <TextView
        android:id="@+id/textview_second3"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_corner"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button_second3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_second4"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints"
        tools:text="@tools:sample/cities" />

    <TextView
        android:id="@+id/textview_second4"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:background="@drawable/rounded_corner"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textview_second2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_second"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints"
        tools:text="@tools:sample/cities" />

    <Button
        android:id="@+id/button_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/previous"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview_second"
        app:layout_constraintVertical_bias="0.907" />

</androidx.constraintlayout.widget.ConstraintLayout>